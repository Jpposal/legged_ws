    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="robot2_base_link"/>
    <child link="robot2_base"/>
  </joint>

  <joint name="robot2_world_fixed" type="fixed">
    <parent link="world"/>
    <child link="robot2_base_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

