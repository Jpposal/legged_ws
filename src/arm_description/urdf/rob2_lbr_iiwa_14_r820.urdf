    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1.5479"/>
      <inertia ixx="0.004" ixy="0.0" ixz="0.0"
               iyy="0.004" iyz="0.0"
               izz="0.004"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
