    </joint>
    <actuator name="robot2_motor_a4">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="robot2_trans_joint_a5">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="robot2_joint_a5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="robot2_motor_a5">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="robot2_trans_joint_a6">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="robot2_joint_a6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="robot2_motor_a6">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="robot2_trans_joint_a7">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="robot2_joint_a7">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="robot2_motor_a7">
      <hardwareInterface>EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>


  <!-- ========================= -->
  <!-- Gazebo ros_control        -->
  <!-- ========================= -->
  <gazebo>
    <plugin name="robot2_gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/robot2</robotNamespace>
      <controlPeriod>0.001</controlPeriod>
      <legacyModeNS>false</legacyModeNS>
    </plugin>
  </gazebo>


  <!-- Base friction -->
  <gazebo reference="robot2_base_link">
    <material>Gazebo/Gray</material>
    <mu1>10.0</mu1>
    <mu2>10.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
    <fdir1>0 0 0</fdir1>
  </gazebo>


  <!-- ========================= -->
  <!-- Payload                   -->
  <!-- ========================= -->

  <joint name="robot2_payload_joint" type="fixed">
    <parent link="robot2_link_7"/>
    <child link="robot2_payload_link"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
  </joint>

  <link name="robot2_payload_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0"
               iyy="0.01" iyz="0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="robot2_payload_grey">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>


  <!-- ========================= -->
  <!-- acceleration plugin       -->
  <!-- ========================= -->

  <gazebo>
    <plugin name="robot2_link_accel_plugin" filename="libgazebo_link_accel_plugin.so">
      <updateRate>500.0</updateRate>
      <robotNamespace>/robot2</robotNamespace>
      <topicName>/robot2/gazebo/link_accel</topicName>
      <gaussianNoise>0.0</gaussianNoise>
    </plugin>
  </gazebo>

</robot>
