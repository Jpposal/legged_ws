<?xml version="1.0" ?>
<robot name="robot2_kuka_lbr_iiwa_14_r820" xmlns:xacro="http://wiki.ros.org/xacro">

  <!-- ========================= -->
  <!-- World + 偏移避免穿模       -->
  <!-- ========================= -->
  <link name="world"/>

  <!-- robot2 放到 x=1.2 m -->
  <joint name="robot2_world_to_base" type="fixed">
    <parent link="world"/>
    <child link="robot2_base_link"/>
    <origin xyz="1.2 0 0.05" rpy="0 0 0"/>
  </joint>


