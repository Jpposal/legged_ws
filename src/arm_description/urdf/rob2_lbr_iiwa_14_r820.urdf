      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="50"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
               iyy="0.0001" iyz="0.0"
               izz="0.0001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/lbr_iiwa_14_r820/visual/base_link.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/lbr_iiwa_14_r820/collision/base_link.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="robot2_link_1">
    <inertial>
      <origin xyz="0 0 0.027" rpy="0 0 0"/>
      <mass value="3.9478"/>
      <inertia ixx="0.010" ixy="0.0" ixz="0.0"
               iyy="0.010" iyz="0.0"
               izz="0.010"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/lbr_iiwa_14_r820/visual/link_1.dae"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://arm_description/meshes/lbr_iiwa_14_r820/collision/link_1.stl"/>
      </geometry>
    </collision>
  </link>

  <link name="robot2_link_2">
    <inertial>
      <origin xyz="0 0 0.016" rpy="0 0 0"/>
      <mass value="3.7876"/>
      <inertia ixx="0.009" ixy="0.0" ixz="0.0"
               iyy="0.009" iyz="0.0"
               izz="0.009"/>
